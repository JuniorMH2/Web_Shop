<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>borraUsuario</servlet-name>
        <servlet-class>Controller.borraUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>muestraUsuarios</servlet-name>
        <servlet-class>Controller.muestraUsuarios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registro</servlet-name>
        <servlet-class>Controller.registro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Compra_Product</servlet-name>
        <servlet-class>Controller.Compra_Product</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>muestraCursos</servlet-name>
        <servlet-class>Controller.muestraCursos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>borraCurso</servlet-name>
        <servlet-class>Controller.borraCurso</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>borraUsuario</servlet-name>
        <url-pattern>/borraUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Compra_Product</servlet-name>
        <url-pattern>/Compra_Product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>muestraCursos</servlet-name>
        <url-pattern>/muestraCursos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>borraCurso</servlet-name>
        <url-pattern>/borraCurso</url-pattern>
    </servlet-mapping>
</web-app>
